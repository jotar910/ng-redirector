{
  "name": "ng-redirector",
  "description": "A redirector extension for Angular proxy files.",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "ng-redirector",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "<all_urls>",
    "*://*/*",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "activeTab",
    "cookies"
  ]
}
